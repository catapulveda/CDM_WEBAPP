<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <!--
    <welcome-file-list>
        <welcome-file>
            homepage
        </welcome-file>
    </welcome-file-list>
    -->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.cdm.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletHomePage</servlet-name>
        <servlet-class>com.cdm.controller.ServletHomePage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>com.cdm.controller.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Transformadores</servlet-name>
        <servlet-class>com.cdm.controller.TransformadorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLotes</servlet-name>
        <servlet-class>com.cdm.controller.LoteController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/homepage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Transformadores</servlet-name>
        <url-pattern>/listartrafos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLotes</servlet-name>
        <url-pattern>/lotes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
